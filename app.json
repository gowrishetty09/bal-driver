      "package": "com.bal.driver",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#151e2d"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-secure-store", "expo-location"],
    "extra": {
      "apiBaseUrl": "http://192.168.0.144:3000",
      "useMocks": false,
      "googlePlacesApiKey": "AIzaSyDRN3_J4mGdkmWifoACmHBfT68sIvD3kQ8",
      "sosContacts": {
        "police": "100",
        "medical": "108",
        "support": "+1800123456",
        "shareTemplate": "Emergency! Please track me at https://maps.google.com/?q={lat},{lng}"
      },
      "eas": {
